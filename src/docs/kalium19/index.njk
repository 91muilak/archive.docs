<!DOCTYPE html>
<html lang="ru">
	<head>
		{% include "head.njk" %}
	</head>
	<body>
		
		{% include "nav.njk" %}
		{% include "header.njk" %}

		<br>
		<main>
			<div class="container">
				<div class="row">
					{% for name, desc in mixins %}
						<div class="col-12 col-md-6 col-lg-4 mb-3 mb-md-4 mb-lg-0">
							<div class="card">
								<header>
									<span class="card__subtitle">@mixins</span>
									<h4 class="card__title">{{ name }}</h4>
								</header>
								<p class="card__description">{{ desc }}</p>
								<a class="card__link" href="@mixins/{{ name }}" tabindex="0"></a>
							</div>
						</div>
					{% endfor %}
				</div>
			</div>
		</main>
		
	</body>
</html>