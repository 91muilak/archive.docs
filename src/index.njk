<!DOCTYPE html>
<html lang="ru">
	<head>
		{% include "head.njk" %}
	</head>
	<body>
		
		{% include "nav.njk" %}
		{% include "header.njk" %}

		<br>

		<main>
			<div class="container">
				<div class="row">
					{% for id, obj in projects %}
						<div class="col-12 col-md-6 col-lg-4 mb-3 mb-md-4 mb-lg-0">
							<div class="card">
								<h4 class="card__title">{{ obj.name }}</h4>
								<p class="card__description">{{ obj.description }}</p>
								<a class="card__link" href="docs/{{ id }}" tabindex="0"></a>
							</div>
						</div>
					{% endfor %}
				</div>
			</div>
		</main>

	</body>
</html>